{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["./node_modules/decanter/core/src/js/"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "./jsdoc-theme",
    "encoding": "utf8",
    "destination": "./public/docs/js",
    "recurse": true
  }
}
